public class Circle1{
  private double radius = 1.0;
   private String color = "red";
   public circle()
   {
   }
   public circle(double r,String c){
     radius=r;
     color=c;
   }
   public double getRadius()
   {
   return radius;
   }
   public String getColor()
   {
   return color;
   }
   public double getArea()
   {
   double area=Math.PI*radius*radius;
   return area;
   }
}